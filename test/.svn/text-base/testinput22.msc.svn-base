msc {
  1, 2;
  1 box 2 [ label="Line1\\nLine1\nLine2\\nLine2" ];
  1 box 2 [ label="\\n" ];
  1 box 2 [ label="\\nLine1" ];
  1 box 2 [ label="\nLine2" ];
  1 box 2 [ label="\nLine2\\n\n" ];
}

